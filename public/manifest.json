{
  "name": "ShareIT",
  "short_name": "share-it",
  "version": "0.0.1",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "127": "icon128.png"
  },
  "background": {
    "scripts": [
      "socket.io.js",
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_title": "ShareIT by InspireIT"
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "activeTab",
    "storage"
  ]
}
